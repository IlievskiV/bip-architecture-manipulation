model PriorityManagementOperands
	
	port type ActionPort
	port type NoActionPort
	
	atomic type Operand
		export port ActionPort actionO()
		export port NoActionPort noActionO()
	
		place state0, state1
		initial to state0
		
		on actionO from state0 to state1
		on noActionO from state1 to state0
	end
	
	compound type PriorityManagementOperandsRoot
		component Operand B1
		component Operand B2
		component Operand B3
	end
	
	component PriorityManagementOperandsRoot root
end